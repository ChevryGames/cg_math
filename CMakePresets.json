{
  "version": 1,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 19,
    "patch": 1
  },
  "configurePresets": [
    {
      "name": "generator-mingw",
      "hidden": true,
      "generator": "MinGW Makefiles",
      "cacheVariables": {
        "GENERATOR_NAME": {
          "type": "STRING",
          "value": "mingw-makefiles"
        }
      }
    },
    {
      "name": "generator-vs-16-2019",
      "hidden": true,
      "generator": "Visual Studio 16 2019",
      "cacheVariables": {
        "GENERATOR_NAME": {
          "type": "STRING",
          "value": "vs-16-2019"
        }
      }
    },
    {
      "name": "generator-default",
      "hidden": true,
      "inherits": "generator-vs-16-2019"
    },
    {
      "name": "architecture-default",
      "hidden": true,
      "architecture": {
        "value": "x64"
      },
      "cacheVariables": {
        "ARCHITECTURE_NAME": {
          "type": "STRING",
          "value": "x64"
        }
      }
    },
    {
      "name": "build-type-debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "build-type-release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "debug-default",
      "inherits": [
        "generator-default",
        "architecture-default",
        "build-type-debug"
      ],
      "binaryDir": "${sourceDir}/build/${generator}/debug"
    },
    {
      "name": "release-default",
      "inherits": [
        "generator-default",
        "architecture-default",
        "build-type-release"
      ],
      "binaryDir": "${sourceDir}/build/${generator}/release"
    },
    {
      "name": "default",
      "inherits": "debug-default"
    }
  ]
}